{
  "name": "cufc-ui",
  "private": false,
  "version": "0.2.12",
  "main": "dist/cufc-ui.es.js",
  "styles": ".storybook/output.css",
  "types": "index.d.ts",
  "description": "UI components for Cufc",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build-storybook": "npm run build-tailwind && storybook build",
    "build-tailwind": "tailwindcss -i ./.storybook/tailwind.css -o ./.storybook/output.css --watch",
    "storybook": "concurrently \"npm run watch:tailwind\" \"npm run watch:storybook\"",
    "watch:storybook": "storybook dev -p 6006",
    "watch:tailwind": "tailwindcss build ./tailwind/tailwind.css -o ./tailwind/output.css --watch",
    "test": "npx vitest",
    "test:coverage": "npx vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@testing-library/react": "^14.1.2",
    "concurrently": "^8.2.2",
    "date-fns": "^4.0.0-beta.1",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-datepicker": "^7.3.0",
    "react-dom": "^18.2.0",
    "react-google-recaptcha": "^3.1.0",
    "recharts": "^2.12.7",
    "tailwindcss-grid-areas": "^0.1.1",
    "vite-plugin-svgr": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@chromatic-com/storybook": "^1.5.0",
    "@storybook/addon-console": "^3.0.0",
    "@storybook/addon-essentials": "^8.1.5",
    "@storybook/addon-interactions": "^8.1.5",
    "@storybook/addon-links": "^8.1.5",
    "@storybook/addon-mdx-gfm": "^8.1.5",
    "@storybook/blocks": "^8.1.5",
    "@storybook/react": "^8.1.5",
    "@storybook/react-vite": "^8.1.5",
    "@storybook/test": "^8.1.5",
    "@svgr/rollup": "^8.1.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.17",
    "babel-jest": "^29.7.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.1.0",
    "prop-types": "^15.8.1",
    "storybook": "^8.1.5",
    "tailwindcss": "^3.4.1",
    "vite": "^4.2.0",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^0.34.6"
  }
